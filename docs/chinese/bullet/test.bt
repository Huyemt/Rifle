@ name = "bulletModule"
@ version = "-dev";
@ authors = "Huyemt";
@ canBeSelect = true;

@ onLoad {
    println("Bullet模块加载");
    value := "\n";
    n := 10;
    for ( i := 1; i <= n; i += 1) {
        value = value + " " * (n - i) + "*" * (2 * i - 1) + "\n";
    }
    println(value);
}

@ onSelected {
    println("Bullet模块被选用");
}

@ onQuit {
    println("Bullet模块被弃用");
}

@ onDisable {
    println("Bullet模块卸载");
}
